{
    "name": "xoshbin/translatable-select",
    "description": "A Filament Select component with built-in translatable search functionality for Laravel applications using Spatie Laravel Translatable",
    "keywords": [
        "filament",
        "laravel",
        "translatable",
        "select",
        "search",
        "spatie",
        "multi-locale",
        "cross-locale"
    ],
    "homepage": "https://github.com/xoshbin/translatable-select",
    "support": {
        "issues": "https://github.com/xoshbin/translatable-select/issues",
        "source": "https://github.com/xoshbin/translatable-select"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Khoshbin",
            "email": "xoshbin@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "illuminate/contracts": "*",
        "illuminate/database": "*",
        "illuminate/support": "*",
        "spatie/laravel-package-tools": "^1.15.0",
        "spatie/laravel-translatable": "^6.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Xoshbin\\TranslatableSelect\\": "src/",
            "Xoshbin\\TranslatableSelect\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xoshbin\\TranslatableSelect\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xoshbin\\TranslatableSelect\\TranslatableSelectServiceProvider"
            ],
            "aliases": {
                "TranslatableSelect": "Xoshbin\\TranslatableSelect\\Facades\\TranslatableSelect"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
